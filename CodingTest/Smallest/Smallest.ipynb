   "source": [
    "# 제일 작은 수 제거하기\n",
    "def solution(arr):\n",
    "    answer = []\n",
    "    min_val = min(arr)\n",
    "    if len(arr) < 2 :\n",
    "        answer.append(-1)\n",
    "    else :\n",
    "        for i in range(len(arr)):\n",
    "            if arr[i] != min_val:\n",
    "                answer.append(arr[i])\n",
    "    return answer"
   ]
  }
